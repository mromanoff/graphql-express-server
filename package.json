{
  "name": "graphql-express-server",
  "version": "1.0.0",
  "description": "Simple GraphQL Server",
  "main": "server.js",
  "scripts": {
    "dev:server": "nodemon server.js",
    "json:server": "json-server --watch data.json --port 4000",
    "dev": "concurrently \"npm run json:server\" \"npm run dev:server\""
  },
