  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "json-server": "^0.14.2",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "prettier": "^1.16.4"
  }
}
